$(function () {
    UpdateCrt('#dianji')
});
function UpdateCrt(obj) {
    var this_href = window.location.href;
    var reg = /\d{1,}_(\d{1,})\.html/;
    if (reg.test(this_href)) {
        var data = "cmd=updatecrt&id=" + reg.exec(this_href)[1] + "";
        ajaxhelpS(data,
        function (msg) {
            $(obj).text(msg);
            //document.getElementById('dianji').innerText = msg;
        })
    }
}




